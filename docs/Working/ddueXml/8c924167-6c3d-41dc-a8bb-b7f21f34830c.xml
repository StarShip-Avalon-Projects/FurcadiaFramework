    <section address="Section1">
      <title>Dragon Speak Constructor</title>
      <content>
        <section>
          <title>User Configuration</title>
          <content>
            <para>%USERPROFILE%\Local Settings\Application Data\MEP\DSConstructor</para>
            <para>C:\Users\UserName\AppData\Local\MEP\DSConstructor</para>
          </content>
          </section>
        <section>
          <title>Install Path</title>
          <content>
            <para>Windows x86: c:\Program Files\Furcadia\ </para>
            <para>Windows x64: c:\Program Files (x86)\Furcadia\ </para>
          </content>
        </section>
        <section>
          <title>DS Template</title>
          <content>
            <para>%USERPROFILE%\My Documents\Furcadia\Templates </para>
            <para>c:\Documents and Settings\username\My Documents\Furcadia\Templates </para>
          </content>
        </section>
        <section>
          <title>DS Wizard Scripts</title>
          <content>
            <para>%USERPROFILE%\My Documents\Furcadia\Scripts </para>
            <para>c:\Documents and Settings\username\My Documents\Furcadia\Scripts </para>
          </content>
        </section>
