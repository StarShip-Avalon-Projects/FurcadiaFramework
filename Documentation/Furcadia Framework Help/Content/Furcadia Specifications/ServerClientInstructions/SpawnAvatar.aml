              <para>
                Map X Position 
              </para>
            </entry>
          </row>
          <row>
            <entry>
              <para>Y Coordinate</para>
            </entry>
            <entry>
              <para>b220  integer</para>
            </entry>
            <entry>
              <para>2</para>
            </entry>
            <entry>
              <para>
                Map Y Position
              </para>
            </entry>
          </row>
          <row>
            <entry>
              <para>Direction</para>
            </entry>
            <entry>
              <para>b220  integer</para>
            </entry>
            <entry>
              <para>1</para>
            </entry>
